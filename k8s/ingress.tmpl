apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: manhattan-ingress
  annotations:
    kubernetes.io/ingress.class: "gce"  
spec:
  rules:
  - http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: frontend-svc
            port:
              number: 80
